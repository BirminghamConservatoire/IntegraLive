        </xsd:attribute>
        <xsd:attribute name="inLevel" xmlns:rixd="http://www.integralive.org/schemas/2014/rixd">
            <xsd:simpleType>
                <xsd:restriction base="xsd:float">
                    <xsd:minInclusive value="0" />
                    <xsd:maxInclusive value="1" />
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="loudness" xmlns:rixd="http://www.integralive.org/schemas/2014/rixd">
            <xsd:annotation>
                <xsd:documentation>Displays the detected perceptual Loudness level in dbA.</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:restriction base="xsd:integer">
                    <xsd:minInclusive value="0" />
                    <xsd:maxInclusive value="120" />
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="intensity" xmlns:rixd="http://www.integralive.org/schemas/2014/rixd">
            <xsd:annotation>
                <xsd:documentation>Displays the detected intensity level in dbA.</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:restriction base="xsd:integer">
                    <xsd:minInclusive value="0" />
                    <xsd:maxInclusive value="120" />
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="intensityY" xmlns:rixd="http://www.integralive.org/schemas/2014/rixd">
            <xsd:annotation>
                <xsd:documentation>Displays the detected intensity level.</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:restriction base="xsd:float">
                    <xsd:minInclusive value="0" />
                    <xsd:maxInclusive value="120" />
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="loudnessS" xmlns:rixd="http://www.integralive.org/schemas/2014/rixd">
