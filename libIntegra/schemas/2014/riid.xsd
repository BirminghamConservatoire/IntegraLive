                [Tags:complex|processing|delay|granular|panning]
            </xsd:documentation>
        </xsd:annotation>
        <xsd:attribute name="bypass" xmlns:rixd="http://www.integralive.org/schemas/2014/rixd">
            <xsd:annotation>
                <xsd:documentation>Bypass the module processing</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:restriction base="xsd:integer">
                    <xsd:minInclusive value="0" />
                    <xsd:maxInclusive value="1" />
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="inLevel" xmlns:rixd="http://www.integralive.org/schemas/2014/rixd">
            <xsd:annotation>
                <xsd:documentation>Set the input level of the module</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:restriction base="xsd:float">
                    <xsd:minInclusive value="0" />
                    <xsd:maxInclusive value="1" />
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="outLevel" xmlns:rixd="http://www.integralive.org/schemas/2014/rixd">
            <xsd:annotation>
                <xsd:documentation>Set the output level of the module</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:restriction base="xsd:float">
                    <xsd:minInclusive value="0" />
                    <xsd:maxInclusive value="1" />
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="mix" xmlns:rixd="http://www.integralive.org/schemas/2014/rixd">
            <xsd:annotation>
                <xsd:documentation>Control the relative mix of processed and unprocessed sound</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:restriction base="xsd:float">
                    <xsd:minInclusive value="0" />
                    <xsd:maxInclusive value="1" />
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="delayTime" xmlns:rixd="http://www.integralive.org/schemas/2014/rixd">
            <xsd:annotation>
                <xsd:documentation>Control the time by which the input signal is delayed before being used as a source for granular synthesis</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:restriction base="xsd:float">
                    <xsd:minInclusive value="0.001" />
                    <xsd:maxInclusive value="36" />
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="delayVariation" xmlns:rixd="http://www.integralive.org/schemas/2014/rixd">
            <xsd:annotation>
                <xsd:documentation>Random delay time variation for each grain in milliseconds</xsd:documentation>
            </xsd:annotation>
            <xsd:simpleType>
                <xsd:restriction base="xsd:float">
                    <xsd:minInclusive value="0" />
                    <xsd:maxInclusive value="1" />
                </xsd:restriction>
            </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="grainPhase" xmlns:rixd="http://www.integralive.org/schemas/2014/rixd">
